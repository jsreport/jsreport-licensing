{
  "name": "jsreport-licensing",
  "version": "1.1.3",
  "description": "jsreport extension for enterprise license verification ",
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "test": "standard"
  },
  "homepage": "https://github.com/jsreport/jsreport-licensing",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-licensing.git"
  },
  "keywords": [
    "jsreport"
  ], 
  "dependencies": {
    "bluebird": "3.4.6",
    "in-publish": "2.0.0",
    "mkdirp": "0.5.1",
    "request": "2.79.0"
  },
  "author": "Jan Blaha",
  "standard": {
    "ignore": [
      "main.js",
      "jsreport.config.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.11.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-babel": "4.0.0",
    "jsreport-studio-dev": "1.0.2",
    "standard": "8.6.0"
  }
}
